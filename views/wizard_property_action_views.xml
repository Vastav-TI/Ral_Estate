<odoo>
    <record id="view_property_action_wizard_form" model="ir.ui.view">
        <field name="name">property.action.wizard.form</field>
        <field name="model">property.action.wizard</field>
        <field name="arch" type="xml">
            <form string="Action Required">
                <header>
                    <button name="action_save"
                            string="Save"
                            type="object"
                            class="oe_highlight"/>
                    <button name="action_return"
                            string="Return"
                            type="object"/>
                    <button string="Cancel"
                            class="oe_link"
                            special="cancel"/>
                </header>
                <sheet>
                    <h1>Do you want to save or return?</h1>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
